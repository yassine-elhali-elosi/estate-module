{"input_prompt": "List all opportunities in \"new\" state and that are in my pipeline", "output_prompt": "opportunities_in_new = self.env['crm.lead'].search([('type', '=', 'opportunity'),('stage_id.name', '=', 'New'),('user_id', '=', self.env.user.id)])"}
{"input_prompt": "List all opportunities that are won", "output_prompt": "won_opportunities = self.env['crm.lead'].search([('type', '=', 'opportunity'), ('stage_id.name', '=', 'Won')])"}
{"input_prompt": "list all opportunities that are won and in my pipeline", "output_prompt": "opportunities_won_in_my_pipeline = self.env['crm.lead'].search([('type', '=', 'opportunity'),('stage_id.is_won', '=', True),('user_id', '=', self.env.user.id)])"}
{"input_prompt": "list all projects that have \"Internal\" tag", "output_prompt": "internal_projects = self.env['project.project'].search([('stage_id.is_internal', '=', True), ('stage_id.name', '=', 'Internal')])"}
{"input_prompt": "Which product has the most inventory stock?", "output_prompt": "product_with_most_stock = self.env['product.product'].search([('qty_available', '>', 0)], order='qty_available desc', limit=1)"}
